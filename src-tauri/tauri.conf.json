{
  "productName": "fuyun_tools",
  "version": "0.3.2",
  "identifier": "com.clipboard-manager.fuyun",
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devUrl": "http://localhost:5173",
    "frontendDist": "../src/dist"
  },
  "app": {
    "windows": [
      {
        "label": "clipboard",
        "title": "fuyun_tools",
        "url": "clipboard.html",
        "visible": false,
        "decorations": false,
        "shadow": false,
        "transparent": true,
        "alwaysOnTop": true,
        "skipTaskbar": true,
        "focus": true,
        "center": false,
        "resizable": true,
        "maximizable": false,
        "minimizable": false
      },
      {
        "label": "selection_toolbar",
        "title": "fuyun_tools",
        "url": "selection_toolbar.html",
        "visible": false,
        "resizable": false,
        "decorations": false,
        "transparent": true,
        "alwaysOnTop": true,
        "skipTaskbar": true,
        "focus": false,
        "acceptFirstMouse": true
      },
      {
        "label": "settings",
        "title": "设置",
        "url": "settings.html",
        "visible": false,
        "resizable": true,
        "decorations": true,
        "transparent": false,
        "alwaysOnTop": false,
        "skipTaskbar": false,
        "focus": true,
        "width": 850,
        "height": 700,
        "center": true
      }
    ],
    "security": {
      "csp": "default-src 'self'; script-src 'self'; style-src 'self' 'unsafe-inline'; connect-src 'self' tauri://ipc; img-src 'self' data:; font-src 'self'; object-src 'none';"
    },
    "withGlobalTauri": false
  },
  "plugins": {
    "updater": {
      "active": true,
      "windows": {
        "installMode": "basicUi"
      },
      "endpoints": [
        "https://github.com/zRq1351/fuyun_tools/releases/latest/download/latest.json"
      ],
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IDExODM0OUY1NDFCMTQ5N0MKUldSOFNiRkI5VW1ERVo3SVo2c09NUSs2TU9QdlJkZjZxTHB3d1NKOEtQZmZVWUFpNGFmcEFYSnYK"
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/64x64.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "createUpdaterArtifacts": true
  }
}
